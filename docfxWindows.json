{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": ".\\main",
          "files": [
            "main.csproj"
          ]
        }
      ],
      "output": "api",
      "includePrivateMembers": true,
      "shouldSkipMarkup": false,
      "filter": "filterConfig.yml"
    }
  ],
  "build": {
    "content": [
      {
        "files": "**/*.{md,yml}",
        "exclude": [
          "_site/**",
          "filterConfig.yml"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ]
  }
}