{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "./main",
          "files": [
            "main.csproj"
          ]
        }
      ],
      "output": "api",
      "includePrivateMembers": false,
      "shouldSkipMarkup": false
    }
  ],
  "build": {
    "content": [
      {
        "files": "api/*.yml"
      },
      {
        "files": "toc.yml"
      },
      {
        "files": "index.md"
      }
    ],
    "overwrite": [
      {
        "files": "apidocs/*.md"
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ]
  }
}