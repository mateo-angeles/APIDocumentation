{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": ".\\main",
          "files": [
            "main.csproj"
          ]
        }
      ],
      "output": "api/main",
      "properties": {
        "Configuration": "Release",
        "TargetFramework": "net481",
        "Platform": "x64",
        "CodeAnalysisRuleSet": "",
        "RunCodeAnalysis": "false",
        "EnableNETAnalyzers": "false"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": "**/*.{md,yml}",
        "exclude": "_site/**"
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ]
  }
}